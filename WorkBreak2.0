import time #the time dependency for accessing time
import webbrowser #the webbrowser dependency to access webbrowser
#step-1 make the program wait for 'x' hours without essentially doing nothing
count=0
num_of_breaks=2
time_intervals=60*60
#step-2 make the program open up the webbrowser and play a Youtube video 'x' number of times
print ("This program started on "+time.ctime())
while count<num_of_breaks:
    time.sleep(time_intervals)
    webbrowser.open("https://www.youtube.com/watch?v=4Tr0otuiQuU")
    count=count+1
